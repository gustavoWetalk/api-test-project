{
    "compilerOptions": {
      "target": "ES2017",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true, // Ignora verificações de tipo em arquivos de definição de biblioteca
      "forceConsistentCasingInFileNames": true,
       "outDir": "./dist",
       "types": ["jest", "node"],
       "moduleResolution": "node",
    },
    "include": ["src/**/*", "tests/**/*"],
    "exclude": ["node_modules"]
  }
  